<?xml version="1.0" encoding="UTF-8"?>

<!-- Simworld2020  -->


<PropertyList>

    <path>audiopanel_support.ac</path>

    <animation>
        <type>material</type>
        <object-name>Plane</object-name>
        <emission>
            <red>0.381924</red>
            <green>0.30935844</green>
            <blue>0.161171928</blue>
            <factor-prop>sim/multiplay/generic/float[10]</factor-prop>
        </emission>
    </animation>

    <effect>
        <inherits-from>Aircraft/dhc6/Models/Effects/interior/dhc6-interior</inherits-from>
        <object-name>Plane</object-name>
    </effect>

    <effect>
        <inherits-from>Aircraft/dhc6/Models/Effects/interior/dhc6-flashlight</inherits-from>
        <object-name>Plane</object-name>
    </effect>

    <animation>
        <type>noshadow</type>
    </animation>

    <animation>
        <type>range</type>
        <min-m>0</min-m>
        <max-m>10</max-m>
    </animation>
<!-- Scale animation to increase size of audio panel mount plane   -->	
    <animation>
		<type>scale</type>
		<x-offset>1.2</x-offset>
		<y-offset>1.2</y-offset>
		<z-offset>1.2</z-offset>
	</animation>

</PropertyList>
